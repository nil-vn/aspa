<functions>
<!-- There is an object called Global.Is a collection of methods -->
    <method name="parseInt" type="DINT">
        <arg type="DSTRING" />
        <ast name="METHOD_CALL" text="intval">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="escape" type="DSTRING">
    </method>

    <method name="isFinite" type="DBOOL">
        <arg type="DFLOAT" />
        <ast name="METHOD_CALL" text="is_finite">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="isNaN" type="DBOOL">
        <arg type="DFLOAT" />
        <ast name="METHOD_CALL" text="is_nan">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="parseFloat" type="DFLOAT">
        <arg type="DSTRING" />
        <ast name="METHOD_CALL" text="floatval">
            <ast name="ARGLIST_VALUES">
                <arg index="1" />
            </ast>
        </ast>
    </method>

    <method name="unescape" type="DSTRING">
    </method>

    <property name="Infinity" type="DINT">
    </property>

    <property name="NaN" type="DINT">
    </property>
</functions>
