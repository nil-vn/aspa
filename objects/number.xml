<class name="Number" standalone="true">
    <constructor>
        <arg type="ANY" />
        <ast name="TRANSLATE_ROOT">
            <arg index="1" />
        </ast>
    </constructor>

    <property name="MAX_VALUE" type="DINT">
        <read>
            <ast name="DINT" text="1.7976931348623157E308" />
        </read>
    </property>

    <property name="MIN_VALUE" type="DINT">
        <read>
            <ast name="DINT" text="4.9E-324" />
        </read>
    </property>

    <!--
    <property name="NaN" type="DINT">
        <read>
            <ast name="DINT" text="1.7976931348623157E308" />
        </read>
    </property>
    -->

    <property name="NEGATIVE_INFINITY" type="DINT">
        <read>
            <ast name="DINT" text="-1.7976931348623157E308" />
        </read>
    </property>

    <property name="POSITIVE_INFINITY" type="DINT">
        <read>
            <ast name="DINT" text="1.7976931348623157E308" />
        </read>
    </property>
</class>
